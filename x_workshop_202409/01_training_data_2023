// Training samples for supervised classification, based on Landsat-9, Sentinel-2, MODIS data from 08.06.2023
// Questions? Contact eac0021@uah.edu or emil.cherrington@nasa.gov
// Last updated: 22 Sept. 2024

// var a = require('users/servirbz/bz_mrv:x_workshop_202408/00_training_data_2023');

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

// Puts the 10 sets of training data polygon collections into a single collection (1)
var training_2023 = ee.FeatureCollection([
  ee.Feature(for_bl, {label: 'For_BL', class: 1}), // forest, broadleaf
  ee.Feature(for_nl, {label: 'For_NL', class: 2}), // forest, needleleaf (pine)
  ee.Feature(for_mang, {label: 'For_mang', class: 3}), // forest, mangrove
  ee.Feature(scrub, {label: 'Scrub', class: 4}), // scrub / shrubland
  ee.Feature(savanna, {label: 'Savanna', class: 5}), // savanna
  ee.Feature(ag_pasture, {label: 'Ag_pasture', class: 6}), // agriculture: pasture
  ee.Feature(ag_het, {label: 'Ag_crops', class: 7}), // agriculture: mixed cropland
  ee.Feature(wetland, {label: 'Wetlands', class: 8}), // wetlands
  ee.Feature(water, {label: 'Water', class: 9}), // water bodies
  ee.Feature(urban, {label: 'Urban', class: 10})]); // urban areas

exports.training_2023 = training_2023; // NOTE: Use of the exports.x allows for this to be pulled into other scripts

// Classification based on IPCC's 6 classes (version 1)
var training_2023a = ee.FeatureCollection([
  ee.Feature(forest_land, {label: 'Forest_land', class: 1}), // 
  ee.Feature(cropland, {label: 'Cropland', class: 2}), // 
  ee.Feature(grassland, {label: 'Grassland', class: 3}), // 
  ee.Feature(wetlands, {label: 'Wetlands', class: 4}), // 
  ee.Feature(settlements, {label: 'Settlements', class: 5}), // 
  ee.Feature(other_land, {label: 'Other_land', class: 6})]); // 

exports.training_2023a = training_2023a;

// Classification based on IPCC's 6 classes (version 2)

var training_2023b = ee.FeatureCollection([
  ee.Feature(forest_land, {label: 'Forest_land', class: 1}),
  ee.Feature(ag_het, {label: 'Cropland', class: 2}),
  ee.Feature(ee.FeatureCollection(savanna).merge(scrub).merge(ag_pasture).geometry(), {label: 'Grassland', class: 3}), 
  ee.Feature(wetlands, {label: 'Wetlands', class: 4}),
  ee.Feature(settlements, {label: 'Settlements', class: 5}),
  ee.Feature(other_land, {label: 'Other_land', class: 6})]);


exports.training_2023b = training_2023b;

// Classification based on IPCC's 6 classes (version 3)

var training_2023c = ee.FeatureCollection([
  ee.Feature(ee.FeatureCollection(for_bl).merge(for_nl).merge(for_mang).geometry(), {label: 'Forest_land', class: 1}),
  ee.Feature(ag_het, {label: 'Cropland', class: 2}),
  ee.Feature(ee.FeatureCollection(scrub).merge(savanna).merge(ag_pasture).geometry(), {label: 'Grassland', class: 3}),
  ee.Feature(ee.FeatureCollection(wetland).merge(water).geometry(), {label: 'Wetlands', class: 4}), // 
  ee.Feature(urban, {label: 'Setllements', class: 5}),
  ee.Feature(other_land, {label: 'Other_land', class: 6})]);

exports.training_2023c = training_2023c;

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

// Puts the training data, together with the color palette, and the class names
var training_2023_ = {
"classes":[for_bl, for_nl,for_mang,scrub,savanna,ag_pasture,ag_het, wetland, water, urban],
"palette": ['green','limegreen','teal','olive','magenta','yellow','orange','aqua','blue','red'],
"class_names": ['forest_bl','forest_nl','forest_mg','scrub','savanna','ag_pasture','ag_crops','wetland','water','urban']
};
exports.training_2023_ = training_2023_;

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

// Classes - ONLY FOR REFERENCE
var cl10 = ['forest_bl','forest_nl','forest_mg','scrub','savanna','ag_pasture','ag_crops','wetland','water','urban']; 
exports.cl10 = cl10;

// Color palette for LC maps
var pal10cl = {min:1, max:10, palette: ['green','limegreen','teal','olive','magenta','yellow','orange','aqua','blue','red']};
exports.pal10cl = pal10cl;

var pal6cl = {min:1, max:6, palette: ['green','yellow','moccasin','aqua','red','gray']};
// 1: Forest, 2: Cropland, 3: Grassland, 4: Wetland, 5: Settlements, 6: Other land
exports.pal6cl = pal6cl;

// [Approximate spectral band wavelengths for Landsat / MODIS / Sentinel-2 in m (microns)
var wl = [0.482,0.5615,0.6545,0.865,1.6085,2.2005];
exports.wl = wl;

// Define band visualizations
//var viz = {min: 200, max: 4000, bands: ['B5', 'B4', 'B3']}; // False color (w/o complex histogram stretching)
var viz = {bands: ['B5','B4','B3'], min: [730, 1800, -740], max:[2200, 3800, 2000]}; // This palette applies a nice histogram stretch
exports.viz_543 = viz;
exports.viz = viz;

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

// Defines the Belize area of interest
var bz = ee.FeatureCollection("users/servirbz/aoi/lac/cam/bz/bz_bounds_swbd_buff60m");
exports.bz = bz;

// Land mask defined by Hansen et al. (2013)
var msk = ee.Image('UMD/hansen/global_forest_change_2015').select('datamask').eq(1);
exports.msk = msk;

